    <snippet attachedevent="false" class="scripts.Design_30_30_FollowTarget" classname="scripts.Design_30_30_FollowTarget" desc="Make the actor follow a given actor." description="Make the actor follow a given actor." design="true" drawOrder="0" id="30" name="Follow Target" oid="-1" package="scripts" shared="false" type="actor">
